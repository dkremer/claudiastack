}

.wrap a {
	display: table-cell;
	width: 110px;
	height: 110px;
	text-align: center; /* qui e non su precedente, iCab3 */
	vertical-align: middle;
	border: 1px solid #cdc;
	/*background-color: #ffa;*/
}
.wrap img {
	border: 1px solid #cdc;
	vertical-align: middle;
}

/*\*//*/
* html .wrap a {
	display: block;
}
* html .wrap span {
	display: inline-block;
	vertical-align: middle;
	height: 100%;
	width: 1px;
}
* html .wrap a {
	font-size: 1px;
}
*/
.wrap div {
	border-collapse: collapse; /* Op7 */
}
.clear {
	clear: both;
	margin: 0;
	padding: 1em 0;
}
.wrap a:hover {
	background-color: #dfd;
}
</style>
